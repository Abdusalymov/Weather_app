ile{\n    margin-right: 25px;\n}\n\n@media (min-width: 800px){\n    :root{\n        font-size: 1em;\n    }\n    .wi{\n        fill: white;\n        width: 100px;\n        height: 100px;\n    }\n    .weather_box {\n        display: flex;\n        flex-direction: row; \n        margin: auto;\n        width: 95%;\n        height: 100vh;\n        max-width: 800px;\n        align-items: center;\n        position: relative;\n    }\n    .search_icon_container {\n        display: none;\n    }\n    .search_box {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-end;\n        position: absolute;\n        top: 25px;\n        right: 0;\n    }\n    .citysNamesList{\n        position: relative;\n        background: #fff;\n        list-style-type: none;\n        padding-left: 0;\n    }\n    .citysNamesList li{\n        list-style-type: none;\n        cursor: pointer;\n    }\n    .citysNamesList li span{\n        margin: 0 10px;\n    }\n    .citysNamesList li:hover{\n        background-color: rgba(250, 145, 32, .5); \n    } \n    #dayList{\n        display: flex;\n        justify-content: space-around;\n    }\n    input[type=\"text\"] {\n        padding: 2px;\n        border: none;\n        width: 200px;\n        height: 20px;\n    }\n    .weather_today{\n        flex-shrink: 3;\n        align-items: end;\n    }\n    .days{\n        width: 30%;\n        align-items: end;\n        padding-left: 15px;\n        box-sizing: border-box;\n    }\n    .day{\n        margin-left: 0;\n    }\n    .temperature {\n        font-size: 1.9rem;\n    }\n    .weather_icon{\n        width: 100%;\n        display: flex;\n        justify-content: center;\n    }\n}"},"lineCount":null}},"error":null,"hash":"5711829eb246a39544c08a94379555c9","cacheData":{"env":{}}}